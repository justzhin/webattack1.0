{
  "version": 2,
  "builds": [
    {
      "src": "api/attack.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/status.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/proxy.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/api/attack",
      "dest": "/api/attack.js",
      "methods": ["POST"]
    },
    {
      "src": "/api/status",
      "dest": "/api/status.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/proxy",
      "dest": "/api/proxy.js",
      "methods": ["GET"]
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
