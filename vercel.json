{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/api/attack",
      "dest": "/api/index.js",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "src": "/api/status",
      "dest": "/api/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/status/(?<id>[^/]+)",
      "dest": "/api/index.js?id=$1",
      "methods": ["GET"]
    },
    {
      "src": "/api/system",
      "dest": "/api/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/api",
      "dest": "/api/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|ico|svg))",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ]
}
